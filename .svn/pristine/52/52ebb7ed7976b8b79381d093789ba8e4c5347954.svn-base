package org.bureaumanager.bureaumanagerpro.model.rewardsAndPunishmentsManagement.pojo.dto;

import org.bureaumanager.bureaumanagerpro.model.rewardsAndPunishmentsManagement.pojo.SisoBursary;

public class SisoBursaryDto extends SisoBursary {
    private String classId;
    private String grade;

    public String getClassId() {
        return classId;
    }

    public void setClassId(String classId) {
        if ("".equals(classId))
            this.classId = null;
        else
            this.classId = classId;
    }

    public String getGrade() {
        return grade;
    }

    public void setGrade(String grade) {
        if ("".equals(grade))
            this.grade = null;
        else
            this.grade = grade;
    }
}
