<script>
    layui.config({
        base: '${ctxStatic}/layuiadmin/' //静态资源所在路径
    }).extend({
        index: 'lib/index' //主入口模块
    }).use(['index', 'form'],function(){
        var $ = layui.$
            ,form = layui.form;
        qurryForm1();
        qurryForm2();
        qurryForm3();

        function qurryForm1() {
            $.ajax({
                url: '${ctx}/stu/studentInfo', //模拟接口
                type: 'get',
                success: function(r){
                    form.val("stu-form1", {
                        "userName": r.userName // "name": "value"
                        ,"id": r.id
                        ,"gender": r.gender
                        ,"collegeName": r.collegeName
                        ,"idCard": r.idCard
                        ,"specialtyName": r.specialtyName
                        ,"entryYear": r.entryYear
                        ,"phone": r.phone
                        ,"postalCode": r.postalCode
                        ,"email": r.email
                        ,"nation": r.nation
                        ,"birthdate": r.birthdate
                        ,"studyYears": "3年"
                        ,"politicsStatus": r.politicsStatus
                        ,"nativePlace": r.nativePlace
                        ,"familyAddress": r.familyAddress
                        ,"onShool": r.onShool
                        ,"shoolRoll": r.shoolRoll
                        ,"bankCard": r.bankCard
                    })
                }
            });
        }

        function qurryForm2() {
            $.ajax({
                url: '${ctx}/class/classInfo', //模拟接口
                type: 'get',
                success: function(r){
                    form.val("stu-form2", {
                        "orgName": r.orgName // "name": "value"
                        ,"specialtyName": r.specialtyName
                        ,"className": r.className
                        ,"monitorName": r.monitorName
                        ,"monitorTel": r.monitorTel
                        ,"adviserName": r.adviserName
                        ,"adviserTel": r.adviserTel
                        ,"remark": r.remark
                    })
                }
            });
        }

        function qurryForm3() {
            $.ajax({
                url: '${ctx}/stu/dormInfo', //模拟接口
                type: 'get',
                success: function(r){
                    form.val("stu-form3", {
                        "area": r.area // "name": "value"
                        ,"buildingNum": r.buildingNum
                        ,"floor": r.floor
                        ,"id": r.id
                        ,"orientation": r.orientation
                        ,"leader": r.leader
                        ,"bunkNum": r.bunkNum
                    })
                }
            });
        }

        form.render();
    });
</script>
