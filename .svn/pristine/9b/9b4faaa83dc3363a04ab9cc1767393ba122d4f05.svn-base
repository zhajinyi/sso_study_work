package org.bureaumanager.bureaumanagerpro.model.stuBasicInfo.pojo.dto;

import org.bureaumanager.bureaumanagerpro.model.stuBasicInfo.pojo.SisoClassInfo;
import org.bureaumanager.bureaumanagerpro.sysConfig.pojo.dto.SysEmpInfoDto;
import org.bureaumanager.bureaumanagerpro.sysConfig.pojo.dto.SysOrganizationDto;

public class SisoClassInfoDto extends SisoClassInfo {

    //联合对象
    private SysOrganizationDto organizationDto;
    private SisoSpecialtyDto specialtyDto;
    private SysEmpInfoDto adviserInfo;
    private SysEmpInfoDto monitorInfo;

    public SysOrganizationDto getOrganizationDto() {
        return organizationDto;
    }

    public void setOrganizationDto(SysOrganizationDto organizationDto) {
        this.organizationDto = organizationDto;
    }

    public SisoSpecialtyDto getSpecialtyDto() {
        return specialtyDto;
    }

    public void setSpecialtyDto(SisoSpecialtyDto specialtyDto) {
        this.specialtyDto = specialtyDto;
    }

    public SysEmpInfoDto getAdviserInfo() {
        return adviserInfo;
    }

    public void setAdviserInfo(SysEmpInfoDto adviserInfo) {
        this.adviserInfo = adviserInfo;
    }

    public SysEmpInfoDto getMonitorInfo() {
        return monitorInfo;
    }

    public void setMonitorInfo(SysEmpInfoDto monitorInfo) {
        this.monitorInfo = monitorInfo;
    }

    //拓展字段
    private String orgName;
    private String specialtyName;
    private String adviserName;
    private String adviserTel;
    private String monitorName;
    private String monitorTel;

    public String getOrgName() {
        return orgName;
    }

    public void setOrgName(String orgName) {
        this.orgName = orgName;
    }

    public String getSpecialtyName() {
        return specialtyName;
    }

    public void setSpecialtyName(String specialtyName) {
        this.specialtyName = specialtyName;
    }

    public String getAdviserName() {
        return adviserName;
    }

    public void setAdviserName(String adviserName) {
        this.adviserName = adviserName;
    }

    public String getAdviserTel() {
        return adviserTel;
    }

    public void setAdviserTel(String adviserTel) {
        this.adviserTel = adviserTel;
    }

    public String getMonitorName() {
        return monitorName;
    }

    public void setMonitorName(String monitorName) {
        this.monitorName = monitorName;
    }

    public String getMonitorTel() {
        return monitorTel;
    }

    public void setMonitorTel(String monitorTel) {
        this.monitorTel = monitorTel;
    }
}
